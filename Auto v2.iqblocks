block></statement></block></next></block><block type=\"iq_operator_add\" id=\"T*qeZ3t7rwC(iwIr}3!~\" x=\"-1273\" y=\"286\"><value name=\"NUM1\"><shadow type=\"math_number\" id=\"28q}vI8$;2p;,sVcZAWz\"><field name=\"NUM\"></field></shadow></value><value name=\"NUM2\"><shadow type=\"math_number\" id=\":Zl5]|mA!#vWuF-OV@,`\"><field name=\"NUM\"></field></shadow></value></block></xml>","rconfig":[{"port":[1,6,2],"name":"Drivetrain","customName":false,"deviceType":"Drivetrain","deviceClass":"smartdrive","setting":{"type":"2-motor","wheelSize":"200mm","gearRatio":"7:4","direction":"fwd","gyroType":"smart","width":"173","unit":"mm","wheelbase":"76","wheelbaseUnit":"mm","xOffset":"0","yOffset":"0","thetaOffset":"0"},"triportSourcePort":22},{"port":[9,10],"name":"Catapult","customName":true,"deviceType":"MotorGroup","deviceClass":"motor_group","setting":{"fwd":"forward","rev":"reverse","motor_a_reversed":"false","motor_b_reversed":"false"},"triportSourcePort":22},{"port":[11],"name":"Light","customName":true,"deviceType":"Color","deviceClass":"colorsensor","setting":{},"triportSourcePort":22},{"port":[12],"name":"Detector","customName":true,"deviceType":"Color","deviceClass":"colorsensor","setting":{},"triportSourcePort":22},{"port":[4],"name":"Intake","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse"},"triportSourcePort":22}],"slot":0,"platform":"IQ","sdkVersion":"20210916.19.00.00","appVersion":"2.2.2","fileFormat":"1.0.1","icon":"","targetBrainGen":"First","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START IQ MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END IQ MACROS\n\n\n// Robot configuration code.\nmotor LeftDriveSmart = motor(PORT1, 1.75, false);\nmotor RightDriveSmart = motor(PORT6, 1.75, true);\ngyro DrivetrainGyro = gyro(PORT2, true);\nsmartdrive Drivetrain = smartdrive(LeftDriveSmart, RightDriveSmart, DrivetrainGyro, 200);\n\nmotor CatapultMotorA = motor(PORT9, false);\nmotor CatapultMotorB = motor(PORT10, false);\nmotor_group Catapult = motor_group(CatapultMotorA, CatapultMotorB);\n\ncolorsensor Light = colorsensor(PORT11);\ncolorsensor Detector = colorsensor(PORT12);\nmotor Intake = motor(PORT4, false);\n\nvoid calibrateDrivetrain() {\n  wait(200, msec);\n  Brain.Screen.print(\"Calibrating\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"Gyro\");\n  DrivetrainGyro.calibrate();\n  while (DrivetrainGyro.isCalibrating()) {\n    wait(25, msec);\n  }\n\n  // Clears the screen and returns the cursor to row 1, column 1.\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n}\n\n// Generated code.\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat Distance;\n\nevent Shooting = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  // Find out the Distance Varible\n  Distance = 0.0;\n  Catapult.setStopping(hold);\n  Intake.setStopping(coast);\n  Light.setLight(60, percent);\n  if (Brain.buttonCheck.pressing()) {\n    Drivetrain.setHeading(0.0, degrees);\n    while (true) {\n      if (!Catapult.isSpinning()) {\n        Intake.spin(forward);\n      }\n    wait(20, msec);\n    }\n    while (!(static_cast<float>(Light.brightness()) < 40.0)) {\n      Catapult.spin(forward);\n    wait(20, msec);\n    }\n    Drivetrain.turnFor(right, 15.0, degrees);\n    Drivetrain.driveFor(forward, 8.0, inches);\n    Drivetrain.driveFor(reverse, 8.0, inches);\n    Drivetrain.turnFor(left, 30.0, degrees);\n    Drivetrain.driveFor(forward, 8.0, inches);\n    Drivetrain.turnToRotation(180.0, degrees);\n    Drivetrain.driveFor(reverse, 8.0, inches);\n    Drivetrain.driveFor(forward, (Distance + 100.0), mm);\n    Intake.spinFor(forward, 2.0, turns);\n    Catapult.spinFor(forward, 80.0, degrees);\n    while (!(static_cast<float>(Light.brightness()) > 40.0)) {\n      Catapult.spin(forward);\n    wait(20, msec);\n    }\n    while (!(static_cast<float>(Light.brightness()) < 40.0)) {\n      Catapult.spin(forward);\n    wait(20, msec);\n    }\n    Drivetrain.turn(right);\n    Drivetrain.driveFor(forward, 850.0, mm);\n    // 10 Second Mark\n    Drivetrain.turn(right);\n    Drivetrain.driveFor(forward, 160.0, mm);\n    Drivetrain.turnFor(right, 70.0, degrees);\n    Drivetrain.driveFor(forward, 250.0, mm);\n    Drivetrain.turnToRotation(-90.0, degrees);\n    Drivetrain.driveFor(forward, 4.0, inches);\n    Drivetrain.driveFor(reverse, Distance, mm);\n    Drivetrain.turnToRotation(90.0, degrees);\n    Shooting.broadcast();\n    waitUntil(Catapult.isDone());\n    Drivetrain.turnFor(right, 165.0, degrees);\n    Drivetrain.driveFor(forward, Distance, mm);\n    Drivetrain.driveFor(reverse, Distance, mm);\n    Drivetrain.turnFor(left, 165.0, degrees);\n    Drivetrain.driveFor(forward, 300.0, mm);\n    // 20 Second Mark\n    Drivetrain.driveFor(reverse, 6.0, inches);\n    Drivetrain.turnFor(right, 80.0, degrees);\n    Drivetrain.driveFor(forward, 550.0, mm);\n    Drivetrain.turnToRotation(0.0, degrees);\n    Drivetrain.driveFor(reverse, 550.0, mm);\n    Drivetrain.turnToRotation(90.0, degrees);\n    Drivetrain.driveFor(reverse, (300.0 - Distance), mm);\n    Shooting.broadcast();\n    waitUntil(Catapult.isDone());\n    // Done\n  }\n  return 0;\n}\n\n// \"when I receive Shooting\" hat block\nvoid onevent_Shooting_0() {\n  repeat(2) {\n    while (!(static_cast<float>(Light.brightness()) < 40.0)) {\n      Catapult.spin(forward);\n    wait(20, msec);\n    }\n    Intake.spinFor(forward, 720.0, degrees);\n    Catapult.spinFor(forward, 450.0, degrees);\n    wait(20, msec);\n  }\n}\n\n\nint main() {\n  // Calibrate the Drivetrain Gyro\n  calibrateDrivetrain();\n\n  // register event handlers\n  Shooting(onevent_Shooting_0);\n\n  wait(15, msec);\n  whenStarted1();\n}","target":"Physical"}